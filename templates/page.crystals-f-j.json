    "left_image_tab_section_RkgCM3": {
      "type": "left-image-tab-section",
      "blocks": {
        "richtext_H8Hdbr": {
          "type": "richtext",
          "settings": {
            "title": "Halite",
            "richtext": "<p>Cleansing ✧ Wellbeing ✧ Beauty</p><p>Halite is a powerful stone with a number of specific uses, namely in removing negativity and encouraging wellbeing.</p>",
            "title-ta": "Read more ⇢",
            "title-tb": "Chakra ⇢",
            "title-tc": "Details ⇢",
            "text-ta": "Useful for detaching spiritual entities or those from a higher plane, in turn Halite will help with more positive spiritual development. \n\n<h5>This is beautiful, nurturing stone will promote feelings of love.</h5>\n\nBe careful though, as a salt mineral, Halite will dissolved if placed in water.",
            "image": "shopify://shop_images/pf-b97cc542--CHAKRAS09_5318e4a6-bdfc-462c-a6b1-441691111270.webp",
            "text-tc": "<p><strong>Origin:</strong> United States.<br/><strong>Colours: </strong>Pink.<br/><strong>Helps with: </strong>Dispelling negativity, detaching entities, spiritual knowledge, love, wellbeing.</p>"
          }
        }
