    "left_image_tab_section_8KEqFE": {
      "type": "left-image-tab-section",
      "blocks": {
        "richtext_Kj6JW3": {
          "type": "richtext",
          "settings": {
            "title": "Serpentine",
            "richtext": "<p>Calming ✧ Detoxify ✧ Self-belief</p><p>This ancient stone is capable of balancing our entire chakra system through its gentle, calming and grounding energy.</p>",
            "title-ta": "Read more ⇢",
            "title-tb": "Chakra ⇢",
            "title-tc": "Details ⇢",
            "text-ta": "Serpentine aids in the release of harmful emotions that are difficult to shift, helping to release these heavy feelings and dissolve energetic blocks. \n\n<h5>Serpentine is often used for protection of whatever is precious to us, whether that be physical possessions or our own inner being. </h5>\n\nThis crystal will protect and nurture your core beliefs and allow you to remain true to yourself, bringing a sense of well-being and calm to our lives.",
            "image": "shopify://shop_images/serpentine-1670190817185.webp",
            "text-tc": "<p><strong>Origin: </strong>Worldwide. South Africa, Australia, Austria, Germany, France, Madagascar, Peru and the United Kingdom.<br/><strong>Helps with:</strong> Meditation, transformation, protection, calming, soothing, grounding, self-belief, inner peace, well-being.<br/><strong>Usage tip:</strong> If you are at the very beginning of your meditation practice, hold Serpentine to help quieten your mind.Wear Serpentine to facilitate a calm outcome to an emotional or traumatic discussion.</p>"
          }
        }
      },
      "block_order": [
        "richtext_Kj6JW3"
      ],
      "settings": {
        "image": "shopify://shop_images/screenshot-20221205-at-7_0006_serpentine-1670190834804.webp",
